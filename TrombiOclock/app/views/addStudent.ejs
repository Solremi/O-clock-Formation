<%- include("partials/header") %>
    <main>
        <form method="POST">
            <p>
                <label for="promo">Promo</label>
                <select name="promo_id" id="promo_id">
                    <option disabled selected hidden> Veuiller choisir une promo</option>
                    <%# Boucle pour afficher les promos dans le select %>
                        <% for (const promo of promos) { %>
                            <option value="<%= promo.id %>">
                                <%=promo.name %>
                            </option>
                            <% } %>
                </select>
            </p>
            <p>
                <label>Prénom</label>
                <input type="text" name="first_name" placeholder="Prénom">
            </p>
            <p>
                <label>Nom</label>
                <input type="text" name="last_name" placeholder="Nom">
            </p>
            <p>
                <label>Pseudo Github</label>
                <input type="text" name="github_username" placeholder="PrenomNom">
            </p>
            <p>
                <button>Valider</button>
            </p>
        </form>

    </main>

    <%- include("partials/footer") %>